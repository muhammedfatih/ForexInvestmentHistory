@model ForexHistory.Models.history

@{
    ViewBag.Title = "Delete";
}
@section banner{
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <br />
                <hr />
                <center><h1 class="text-primary">Forex History Dashboard</h1></center>
                <hr />
            </div>
        </div>
    </div>
}
@section featured{
<div class="container-fluid">
    <div class="col-md-3">
        <div class="row-fluid">
            <div class="">
                <div class="well">
                    <h3>Navigation</h3>
                    <ul class="nav nav-list">
                        <li>@Html.ActionLink("Index", "Index", "Home", null, new { @class = "text-success" })</li>
                        <li>@Html.ActionLink("Dashboard", "Index", "Forex", null, new { @class = "text-success" })</li>
                        <li>@Html.ActionLink("Investments", "Index", "HistoryItem", null, new { @class = "text-success" })</li>
                        <li>@Html.ActionLink("Profile", "Profile", "Member", null, new { @class = "text-success" })</li>
                        <li>@Html.ActionLink("Logout", "Logout", "Member", null, new { @class = "text-success" })</li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row-fluid">
            <div class="">
                <div class="well">
                    <h3>Latest Values</h3>
                    <ul class="nav nav-list">
                        <li>@ViewBag.cur1</li>
                        <li>@ViewBag.cur2</li>
                        <li>@ViewBag.cur3</li>
                    </ul>
                </div>
            </div>

        </div>
    </div>
    <div class="col-lg-9 text-danger">
    <h3>Are you sure you want to delete this?</h3>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <p>
            <input type="submit" value="Delete" /> |
            @Html.ActionLink("Back to List", "Index")
        </p>
    }
</div>
</div>
}