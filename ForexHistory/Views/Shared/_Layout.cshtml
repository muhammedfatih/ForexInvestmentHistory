<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/bootstrapcss")
    @Styles.Render("~/Content/agencycss")
    @Styles.Render("~/Content/logincss")
</head>
<body>
    @RenderSection("banner", required: false)
    @RenderSection("featured", required: false)
    @RenderBody()
    @RenderSection("Scripts", required: false);
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrapjs")
    @Scripts.Render("~/bundles/agencyjs")
    <footer id="footer">
        <div class="container" role="contentinfo">
            <div class="row">
                <a href="https://github.com/muhammedfatih/ForexInvestmentHistory"><small>Source Code On Github</small></a><br/>
                <a href="https://github.com/muhammedfatih"><small>My Other Github Repositories</small></a>
            </div>
        </div>
    </footer>
</body>
</html>
